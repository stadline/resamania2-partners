meta {
  name: retrieve club's code
  type: http
  seq: 4
}

get {
  url: https://{{host}}/{{environment}}{{clubId}}
  body: none
  auth: none
}

headers {
  Authorization: Bearer {{jwtToken}}
  x-user-network-node-id: {{networkNodeId}}
  x-user-club-id: {{clubId}}
}

vars:post-response {
  clubCode: res.body?.code
}
