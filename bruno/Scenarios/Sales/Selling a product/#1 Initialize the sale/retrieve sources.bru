meta {
  name: retrieve sources
  type: http
  seq: 4
}

get {
  url: https://{{host}}/{{environment}}/{{client_token}}/referentials/sources
  body: none
  auth: none
}

headers {
  Authorization: Bearer {{jwtToken}}
  x-user-network-node-id: {{networkNodeId}}
  x-user-club-id: {{clubId}}
}

vars:post-response {
  sourceId: res.body["hydra:member"]?.[0]?.["@id"]
}
