meta {
  name: Retrieve coaches
  type: http
  seq: 4
}

get {
  url: https://{{host}}/{{environment}}/{{client_token}}/coaches
  body: none
  auth: none
}

headers {
  authorization: Bearer {{jwtToken}}
  x-user-club-id: {{clubId}}
  x-user-network-node-id: {{networkNodeId}}
}

vars:post-response {
  coachId: res.body["hydra:member"]?.[0]?.["@id"]
}
